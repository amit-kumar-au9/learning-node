<%- include('includes/header.ejs') %>
<body>
    <%- include('includes/navbar.ejs') %>
    <div class="container mt-4 table-responsive">
        <table class="table table-striped table-bordered ">
            <thead class="thead-dark">
                <tr>
                    <th>S.no.</th>
                    <th>Name</th>
                    <th>Email Id</th>
                </tr>
            </thead>
            <tbody>
                <% for(i=0; i<userList.length;i++) {%>
                    <tr>
                        <td>
                            <%= i+1%>
                        </td>
                        <td>
                            <%= userList[i].name%>
                        </td>
                        <td>
                            <%= userList[i].email%>
                        </td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</body>
</html>